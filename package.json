{
  "name": "jhlite",
  "version": "0.0.8-SNAPSHOT",
  "description": "JHipster Lite",
  "homepage": "https://www.jhipster.tech/",
  "bugs": "https://github.com/jhipster/jhipster-lite/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/jhipster/jhipster-lite.git"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/generator-jhipster"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "vue-cli-service build --dest target/classes/static",
    "lint": "vue-cli-service lint",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "serve": "vue-cli-service serve --port 9000",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:nocache": "vue-cli-service test:unit --no-cache",
    "test:unit:watch": "vue-cli-service test:unit --watch"
  },
  "dependencies": {
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "1.2.0",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "husky": "7.0.4",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "12.3.2",
    "prettier": "2.5.1",
    "prettier-plugin-java": "1.6.1",
    "prettier-plugin-packagejson": "2.2.15",
    "typescript": "~4.1.5",
    "vue-jest": "^5.0.0-0"
  },
  "engines": {
    "node": ">=14.18.1"
  },
  "cacheDirectories": [
    "node_modules"
  ]
